{
  "service_detection": {
    "smb": {
      "ports": [139, 445],
      "service_names": ["smb", "netbios", "microsoft-ds"]
    },
    "ldap": {
      "ports": [389, 636, 3268, 3269],
      "service_names": ["ldap", "ldaps", "ms-wbt-server"]
    },
    "web": {
      "ports": [80, 443, 8080, 8443, 8000, 8888],
      "service_names": ["http", "https", "http-proxy", "ssl/http"]
    },
    "rdp": {
      "ports": [3389],
      "service_names": ["rdp", "ms-wbt-server"]
    },
    "ssh": {
      "ports": [22],
      "service_names": ["ssh"]
    },
    "dns": {
      "ports": [53],
      "service_names": ["domain", "dns"]
    },
    "kerberos": {
      "ports": [88],
      "service_names": ["kerberos", "kerberos-sec"]
    },
    "winrm": {
      "ports": [5985, 5986],
      "service_names": ["winrm", "wsman"]
    },
    "mssql": {
      "ports": [1433],
      "service_names": ["mssql", "ms-sql-s"]
    }
  },
  "enumeration_commands": {
    "smb": {
      "shares": "netexec smb {ip} -u '' -p '' --shares",
      "version": "netexec smb {ip}",
      "null_session": "netexec smb {ip} -u '' -p '' --rid-brute",
      "users": "netexec smb {ip} -u guest -p '' --users"
    },
    "ldap": {
      "basic": "netexec ldap {ip}",
      "anonymous": "netexec ldap {ip} -u '' -p ''",
      "domain": "netexec ldap {ip} -u '' -p '' --trusted-for-delegation",
      "users": "netexec ldap {ip} -u '' -p '' --users"
    },
    "web": {
      "headers": "curl -I -k --connect-timeout 10 {protocol}://{ip}:{port}/",
      "directory_busting": "gobuster dir -u {protocol}://{ip}:{port}/ -w /usr/share/wordlists/dirb/common.txt -t 50 -q",
      "nikto": "nikto -h {ip}:{port} -Format txt"
    }
  },
  "nmap_command": "nmap -Pn -n {ip} -sC -sV -oN nmap_{ip}.txt",
  "wordlists": {
    "directories": [
      "/usr/share/wordlists/dirb/common.txt",
      "/usr/share/wordlists/dirbuster/directory-list-2.3-medium.txt"
    ],
    "usernames": [
      "/usr/share/wordlists/seclists/Usernames/top-usernames-shortlist.txt"
    ]
  }
}
